<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd" default-lazy-init="false">
    <!--不进行自动装配 通过ref元素定义-->
    <bean id="personAutowiringNo" class="com.spring.ioc.bean.initBeans.Person" lazy-init="false">
        <property name="car" ref="carBean"/>
    </bean>
<!--定义两个Car类型的bean， personAutowiringByName使用自动装配byType, 如果定义的Car没有使用lazy-init属性的话
在启动的过程中就会报错，所有的用例都不执行，如果使用了lazy-init，则执行到具体使用到personAutowiringByName对象的时候才报错-->
    <!--<bean id="carBean" class="com.spring.ioc.bean.initBeans.Car" lazy-init="true"></bean>-->
    <!--<bean id="car" class="com.spring.ioc.bean.initBeans.Car" lazy-init="true"></bean>-->
    <bean id="car" class="com.spring.ioc.bean.initBeans.Car" lazy-init="false"/>
    <bean id="carBean" class="com.spring.ioc.bean.initBeans.Car" lazy-init="false"/>

    <!-- 自动装配 byType-->
    <bean id="personAutowiringByType" class="com.spring.ioc.bean.initBeans.Person" autowire="byType" lazy-init="false"/>

    <!-- 自动装配 byName-->
    <bean id="personAutowiringByName" class="com.spring.ioc.bean.initBeans.Person" lazy-init="false" />
</beans>